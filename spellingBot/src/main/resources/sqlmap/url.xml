<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.naver.mapper.urlMapper">
	<resultMap type="com.naver.dto.url" id="url">
		<result property="real_url" column="real_url" />
		<result property="short_url" column="short_url" />
	</resultMap>

<insert id="urlAdd" parameterType="com.naver.dto.url">
     INSERT INTO 
     	url(real_url,short_url)
     VALUES(#{real_url}, #{short_url})
   </insert>
   	
</mapper>